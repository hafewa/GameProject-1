using System;
using System.Collections.Generic;
using LuaInterface;

public class SData_ArmyCardExp_C : MonoEX.Singleton<SData_ArmyCardExp_C>
{
    public void setData(LuaTable table1, LuaTable table2)
    {
        var head = new string[table1.Length];
        SDataUtils.dealTable(table1, (Object o1, Object o2) =>
        {
            head[(int)(double)o1 - 1] = (string)o2;
        });
        SDataUtils.dealTable(table2, (Object o1, Object o2) =>
        {
            ArmyCardExp_CInfo dif = new ArmyCardExp_CInfo();
            SDataUtils.dealTable((LuaTable)o2, (Object o11, Object o22) =>
            {
                switch (head[(int)(double)o11 - 1])
				{
					case "CardLevel": dif.CardLevel = (short)(double)o22; break;
					case "CardExp": dif.CardExp = (int)(double)o22; break;
                }
            });
            if (Data.ContainsKey(dif.CardLevel))
                MonoEX.Debug.Logout(MonoEX.LOG_TYPE.LT_ERROR, "重复的ID：" + dif.CardLevel.ToString());
            Data.Add(dif.CardLevel, dif);
        });
    }

    public ArmyCardExp_CInfo GetDataOfID(int Id)
    {
        if (!Data.ContainsKey(Id)) throw new Exception(String.Format("ArmyCardExp_CInfo::GetDataOfID() not found data  Id:{0}", Id));
        return Data[Id];
    }

    internal Dictionary<int, ArmyCardExp_CInfo> Data = new Dictionary<int, ArmyCardExp_CInfo>();
}


public struct ArmyCardExp_CInfo
{
	 /// <summary>
	 ///1
	 /// </summary>
	public short CardLevel;
	 /// <summary>
	 ///0
	 /// </summary>
	public int CardExp;
}
