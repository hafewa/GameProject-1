using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using UniLua;
using System.Collections;
public class Module_DOTweenLua : IModule
{
    public   IEnumerator Init() 
    {
        NameFuncPair[] define = new NameFuncPair[]
		{     
            new NameFuncPair( LuaTweenLib.LIB_NAME,LuaTweenLib.OpenLib),   
            new NameFuncPair( LuaDoTweenLib.LIB_NAME,LuaDoTweenLib.OpenLib),   
            new NameFuncPair( LuaSequenceLib.LIB_NAME,LuaSequenceLib.OpenLib),   
		};

        ILuaState lua = LuaRoot._Lua;

        for (var i = 0; i < define.Length; ++i)
        {
            lua.L_RequireF(define[i].Name, define[i].Func, true);
            lua.Pop(1);
        }

        yield break;
    }
}
