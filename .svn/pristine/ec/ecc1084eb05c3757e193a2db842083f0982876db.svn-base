using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using UniLua;
using UnityEngine;

 
class LuaCMUIPlayTweenLib
{
    class CMUIPlayTweenInfo : ICoreComponentInfo
    {
        public Type type { get { return typeof(UIPlayTween); } }
        public string name { get { return LuaCMUIPlayTweenLib.LIB_NAME; } }
        public void newLuaObj(ILuaState lua, Component cmp)
        {
            LuaCMUIPlayTweenLib._wrap(lua, cmp as UIPlayTween);
        }
    }

    static LuaClassRegHelper m_ClassRegHelper = new LuaClassRegHelper();
    //static LuaMemberHelper m_MemberHelper = new LuaMemberHelper();

    public const string LIB_NAME = "UIPlayTween";
    public static int OpenLib(ILuaState lua)
    {
        //注册成员函数
        NameFuncPair[] member_define = new NameFuncPair[]
        { 
            new NameFuncPair("SetTweenTarget",M_SetTweenTarget),
            new NameFuncPair("GetTweenTarget",M_GetTweenTarget),

            new NameFuncPair("SetEnable",M_SetEnable),
            new NameFuncPair("GetEnable",M_GetEnable),

            new NameFuncPair("SetGroupID",M_SetGroupID),
            new NameFuncPair("GetGroupID",M_GetGroupID),
        };

        NameFuncPair[] operator_define = new NameFuncPair[]
        {
           
        };

        m_ClassRegHelper.RegMembers(lua, member_define, operator_define);


        //注册静态成员
        NameFuncPair[] static_define = new NameFuncPair[]
        {
            
        };

        //注册静态属性
        NameValuePair[] static_property = new NameValuePair[]
        {
            new NameValuePair("Name",LIB_NAME),
        };
        m_ClassRegHelper.RegStatics(lua, static_define, static_property);

        LuaGameObjectLib.RegComponent(new CMUIPlayTweenInfo());
        return 1;
    }


    public static void _wrap(ILuaState lua, UIPlayTween comp)
    {
        LuaOpenLibs._wrapComponent(lua, comp, LIB_NAME, m_ClassRegHelper);
    }

    public static UIPlayTween Lua2Obj(ILuaState lua, int index)
    {
       LuaMemberHelper.CheckClass(lua, index);

       return LuaMemberHelper.GetObject<UIPlayTween>(lua, index, LuaOpenLibs.OBJ_HANDELNAME);
    }


    static int M_SetTweenTarget(ILuaState lua)
    {
        //in self,gameObject
        //out nil
        UIPlayTween self = Lua2Obj(lua, 1);
        GameObject gameObj = LuaGameObjectLib.Lua2Obj(lua, 2);
        self.tweenTarget = gameObj;
        return 0;
    }

    static int M_GetTweenTarget(ILuaState lua)
    {
        //in self
        //out GameObject
        UIPlayTween self = Lua2Obj(lua, 1);
        LuaGameObjectLib._wrap(lua,self.tweenTarget);
        return 1;
    }
    static int M_SetEnable(ILuaState lua)
    {
        //in self,bool
        //out nil
        UIPlayTween self = Lua2Obj(lua, 1);
        bool isEnable = lua.ToBoolean(2);
       
        //if (self != null)
            self.enabled = isEnable;
        //else
        //    Debug.LogError("luaCMUIlayTweenLib M_SetEnable self is null");
        return 0;
    }

    static int M_GetEnable(ILuaState lua)
    {
        //in self
        //out bool
        UIPlayTween self = Lua2Obj(lua, 1);
        //if (self != null)
            lua.PushBoolean(self.enabled);
        //else
        //    Debug.LogError("luaCMUIlayTweenLib M_GetEnable self is null");
        return 1;
    }
    static int M_SetGroupID(ILuaState lua)
    {
        //in self,int
        //out nil
        UIPlayTween self = Lua2Obj(lua, 1);
        int id = lua.L_CheckInteger(2);
        self.tweenGroup = id;
        return 0;
    }

    static int M_GetGroupID(ILuaState lua)
    {
        //in self
        //out int
        UIPlayTween self = Lua2Obj(lua, 1);
        lua.PushInteger(self.tweenGroup);
        return 1;
    }
}
 