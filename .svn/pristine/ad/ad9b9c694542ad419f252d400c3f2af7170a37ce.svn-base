using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using UniLua;
using UnityEngine;


class LuaCMQKItemSlotLibs
{
    class CMQKItemSlotInfo : ICoreComponentInfo
    {
        public Type type { get { return typeof(QKItemSlot); } }
        public string name { get { return LuaCMQKItemSlotLibs.LIB_NAME; } }
        public void newLuaObj(ILuaState lua, Component cmp)
        {
            LuaCMQKItemSlotLibs._wrap(lua, cmp as QKItemSlot);
        }
    }

    static LuaClassRegHelper m_ClassRegHelper = new LuaClassRegHelper();

    public const string LIB_NAME = "CMQKItemSlot";


    public static int OpenLib(ILuaState lua)
    {
        //注册成员函数
        NameFuncPair[] member_define = new NameFuncPair[]
        {
            new NameFuncPair("Reposition",M_Reposition), 
        };
         

        NameFuncPair[] operator_define = new NameFuncPair[]
        {
           
        };

        m_ClassRegHelper.RegMembers(lua, member_define, operator_define);


        //注册静态成员
        NameFuncPair[] static_define = new NameFuncPair[]
        {
           
        };
        //注册静态属性
        NameValuePair[] static_property = new NameValuePair[]
        {
            new NameValuePair("Name",LIB_NAME),
        };
        m_ClassRegHelper.RegStatics(lua, static_define, static_property);

        LuaGameObjectLib.RegComponent(new CMQKItemSlotInfo());

 

        return 1;
    }



    public static void _wrap(ILuaState lua, QKItemSlot comp)
    {
        LuaOpenLibs._wrapComponent(lua, comp, LIB_NAME, m_ClassRegHelper);
    }

    public static QKItemSlot Lua2Obj(ILuaState lua, int index)
    {

       LuaMemberHelper.CheckClass(lua, index);

       return LuaMemberHelper.GetObject<QKItemSlot>(lua, index, LuaOpenLibs.OBJ_HANDELNAME);
    }



    static int M_Reposition(ILuaState lua)
    {
        //in self,pos
        //out nil
        var self = Lua2Obj(lua, 1);
        self.Reposition();
        return 0;
    }
     
}
 
