using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using UniLua;

public class LuaMemData
{
  //  public const string LIB_NAME = "_G"; 

    public static void OpenLib(ILuaState lua)
    {
        
        NameFuncPair[] define = new NameFuncPair[]
			{
				new NameFuncPair( "MemData", 	S_MemData	), 
			};
         
        lua.GetGlobal("_G");
        for (int i = 0; i < define.Length; i++)
        {
            lua.PushString(define[i].Name);
            lua.PushCSharpFunction(define[i].Func);
            lua.RawSet(-3);
        }
        lua.Pop(1);  
    }

    static int S_MemData(ILuaState lua)
    {
        //in
        //out QKJsonDoc
        LuaQKJsonLib._wrap(lua, g_JsonDoc);
        return 1;
    }


    static QK_JsonValue_Map g_JsonDoc = new QK_JsonValue_Map();
}