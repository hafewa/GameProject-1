syntax = "proto2";
import "role_pb.proto";


message SelectRole {
    required  Role  user          =1;
}


message EquipLvlup {
    required  Equip  equip          =1;
}

message EquipLock {
    required  Equip  equip          =1;
}

message EquipRepair {
    required  Equip  equip          =1;
}

message EquipRemake {
    required  Equip  equip          =1;
}

message EquipConfirmRemake {
    required  Equip  equip          =1;
}


