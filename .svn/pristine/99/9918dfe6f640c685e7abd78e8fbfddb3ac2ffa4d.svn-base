using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using UniLua;
using UnityEngine;

class LuaCMUIHeroBanshenLib
{
    class CMUIHeroBanshenInfo : ICoreComponentInfo
    {
        public Type type { get { return typeof(UIHeroBanshen); } }
        public string name { get { return LuaCMUIHeroBanshenLib.LIB_NAME; } }
        public void newLuaObj(ILuaState lua, Component cmp)
        {
            LuaCMUIHeroBanshenLib._wrap(lua, cmp as UIHeroBanshen);
        }
    }

    static LuaClassRegHelper m_ClassRegHelper = new LuaClassRegHelper();

    public const string LIB_NAME = "CMUIHeroBanshen";


    public static int OpenLib(ILuaState lua)
    { 
        //注册成员函数
        NameFuncPair[] member_define = new NameFuncPair[]
        { 
            new NameFuncPair("SetIcon",M_SetIcon), 
        };

        NameFuncPair[] operator_define = new NameFuncPair[]
        {
           
        };

        m_ClassRegHelper.RegMembers(lua, member_define, operator_define);


        //注册静态成员
        NameFuncPair[] static_define = new NameFuncPair[]
        {
           
            
        };
        //注册静态属性
        NameValuePair[] static_property = new NameValuePair[]
        {
            new NameValuePair("Name",LIB_NAME),
        };
        m_ClassRegHelper.RegStatics(lua, static_define, static_property);

        LuaGameObjectLib.RegComponent(new CMUIHeroBanshenInfo());

        return 1;
    }

    public static void _wrap(ILuaState lua, UIHeroBanshen comp)
    {
        LuaOpenLibs._wrapComponent(lua, comp, LIB_NAME, m_ClassRegHelper);
    }

    public static UIHeroBanshen Lua2Obj(ILuaState lua, int index)
    {
       LuaMemberHelper.CheckClass(lua, index);

       return LuaMemberHelper.GetObject<UIHeroBanshen>(lua, index, LuaOpenLibs.OBJ_HANDELNAME);
    }

    static int M_SetIcon(ILuaState lua)
    {
        //in self,heroID,isZhuansheng
        //out 
        var self = Lua2Obj(lua, 1);
        var heroID = lua.L_CheckInteger(2);
        bool isZhuansheng = lua.ToBoolean(3);

        self.SetIcon(heroID, isZhuansheng);

        return 0;
    } 
}

