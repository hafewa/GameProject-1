using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using UniLua;

class LuaBuffMathInfoLibs
{
    static LuaClassRegHelper m_ClassRegHelper = new LuaClassRegHelper();


    public const string LIB_NAME = "BuffMathInfo";

    public static int OpenLib(ILuaState lua)
    {

        //注册成员函数
        NameFuncPair[] member_define = new NameFuncPair[]
        {
            new NameFuncPair("AtcMB",M_AtcMB), 
            new NameFuncPair("AtcID",M_AtcID),  
        };

        NameFuncPair[] operator_define = new NameFuncPair[]
        {
           
        };

        m_ClassRegHelper.RegMembers(lua, member_define, operator_define);


        //注册静态成员
        NameFuncPair[] static_define = new NameFuncPair[]
        { 
        };
        m_ClassRegHelper.RegStatics(lua, static_define);

        return 1;
    }



    public static void _wrap(ILuaState lua, BuffMathInfo obj)
    {
        NameValuePair[] propertys = new NameValuePair[]
        {
            new NameValuePair(LuaOpenLibs.OBJ_HANDELNAME,obj),
        };

        m_ClassRegHelper.InstanceLuaClass(lua, propertys);
    }

    public static BuffMathInfo Lua2Obj(ILuaState lua, int index)
    {
        LuaMemberHelper.CheckClass(lua, index);

        return LuaMemberHelper.GetObject<BuffMathInfo>(lua, index, LuaOpenLibs.OBJ_HANDELNAME);
    }

    static int M_AtcMB(ILuaState lua)
    {
        //in self
        //out int
        BuffMathInfo self = Lua2Obj(lua, 1);
        lua.PushString(self.AtcMB);
        return 1;
    }

    static int M_AtcID(ILuaState lua)
    {
        //in self
        //out int
        BuffMathInfo self = Lua2Obj(lua, 1);
        lua.PushInteger(self.AtcID);
        return 1;
    }
     
}

