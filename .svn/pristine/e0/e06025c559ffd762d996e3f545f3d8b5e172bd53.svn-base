using UnityEngine;
using System.Collections;
using QKFrameWork.CQKCommand;

/// <summary>
/// 游戏中使用的命令
/// </summary>
public static class GameCommand
{
    /// <summary>
    /// 请求登录
    /// FromType:来源的账号体系 AccountFromType
    /// Account:账号
    /// Password:密码
    /// </summary>
    public static QKCommand RequestLogin = new QKCommand("RequestLogin");

    /// <summary>
    /// 请求注册账号
    /// FromType:来源的账号体系 AccountFromType
    /// Account:账号
    /// Password:密码
    /// </summary>
    public static QKCommand RequestRegist = new QKCommand("RequestRegist");

    /// <summary>
    /// 快速登录
    /// </summary>
    public static QKCommand QuickLogin = new QKCommand("QuickLogin");

    /// <summary>
    /// 显示充值页
    /// 
    /// </summary>
    public static QKCommand ShowPayUI = new QKCommand("ShowPayUI");
}
