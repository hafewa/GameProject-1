using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using UniLua;
using System.Collections;
public class Module_EasyTouchLua : IModule
{
    public   IEnumerator Init() 
    {
        NameFuncPair[] define = new NameFuncPair[]
		{     
            new NameFuncPair( LuaCMEasyTouchLib.LIB_NAME,LuaCMEasyTouchLib.OpenLib),  
            new NameFuncPair( LuaGestureLib.LIB_NAME,LuaGestureLib.OpenLib),  
            new NameFuncPair( LuaCMETCJoystickLib.LIB_NAME,LuaCMETCJoystickLib.OpenLib),  
            new NameFuncPair( LuaCMETCJoystickEventsLib.LIB_NAME,LuaCMETCJoystickEventsLib.OpenLib),  

		};

        ILuaState lua = LuaRoot._Lua;

        for (var i = 0; i < define.Length; ++i)
        {
            lua.L_RequireF(define[i].Name, define[i].Func, true);
            lua.Pop(1);
        }

        yield break;
    }
}
